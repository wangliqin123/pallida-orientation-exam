@model List<SpringBootApp.Models.LicencePlate>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <table>
        <tr>
            <td>ID</td>
            <td>Plate</td>
            <td>Car Brand</td>
            <td>Car Model</td>
            <td>Car Color</td>
            <td>Year</td>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Plate</td>
                <td>@item.CarBrand</td>
                <td>@item.CarModel</td>
                <td>@item.Color</td>
                <td>@item.Year</td>               
            </tr>
        }
    </table>
    <h1>Search for plate</h1>
    <form action="/search" method="get">
        <h2>Plate: </h2>
        <label for="Plate"></label>
        <input type="text" name="Plate" />
        <br><button type="submit">Search</button>
    </form>
</body>
</html>
